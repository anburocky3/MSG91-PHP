{
    "name": "anburocky3/msg91-php",
    "description": "Send SMS seamlessly fast with the help of MSG91 Provider in PHP.",
    "keywords": [
        "anburocky3",
        "msg91 php",
        "msg91 otp",
        "msg91 android",
        "msg91 library",
        "php sms india"
    ],
    "homepage": "https://github.com/anburocky3/msg91-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Anbuselvan",
            "email": "15264938+anburocky3@users.noreply.github.com",
            "role": "Developer",
            "homepage": "https://anburocky3.github.io"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Anburocky3\\Msg91\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anburocky3\\Msg91\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
